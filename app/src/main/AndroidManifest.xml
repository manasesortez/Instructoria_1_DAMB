<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.amtodev.calculator_metric">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_android"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_android"
        android:supportsRtl="true"
        android:theme="@style/Theme.Calculator_metric"
        tools:ignore="AllowBackup">
        <activity
            android:name=".acelerometro"
            android:theme="@style/calculator2"
            android:exported="true" />
        <activity
            android:name=".numberMayor"
            android:theme="@style/calculator2"
            android:exported="true" />
        <activity
            android:name=".Temperatura"
            android:theme="@style/calculator2"
            android:exported="true" />
        <activity
            android:name=".calculadora"
            android:theme="@style/calculator2"
            android:exported="true" />
        <activity
            android:name=".informacion"
            android:theme="@style/calculator2"
            android:exported="true" />
        <activity
            android:name=".notificacion"
            android:theme="@style/calculator2"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/calculator2">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Calculator"
            android:theme="@style/calculator2" />
    </application>

</manifest>